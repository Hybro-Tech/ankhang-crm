<% if @dashboard_view == "call_center" %>
  <%= render "call_center_dashboard" %>
<% elsif @dashboard_view == "call_center_overview" %>
  <%= render "call_center_overview" %>
<% elsif @dashboard_view == "sale" %>
  <%= render "sale_dashboard" %>
<% elsif @dashboard_view == "cskh" %>
  <%= render "cskh_dashboard" %>
<% else %>
  <%# Admin / Generic Dashboard %>
  <%= render "admin_dashboard" %>
<% end %>
